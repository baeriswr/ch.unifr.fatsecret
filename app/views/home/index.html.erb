<h3>Home</h3>
<h1>Try FatSecret Food Search</h1>
<%= form_tag fatsecret_path %>
<%= label_tag(:search_expression, "Search for food:") %>
<%= text_field_tag(:search_expression) %>
<%= hidden_field_tag 'method', 'foods.search' %>
<%= submit_tag("Search") %>


<% @users.each do |user| %>
    <p>User: <%= link_to user.name, user %></p>
<% end %>
